<%= simple_form_for(@port, html: {class: "form-horizontal"}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :currency %>
    <%= f.association :tax %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
